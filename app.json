{
  "expo": {
    "name": "sAPP Factory",
    "slug": "sappisfactory",
    "scheme": "sappisfactory",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "newArchEnabled": true,
    "backgroundColor": "#000",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.sAPPisfactory",
      "userInterfaceStyle": "dark",
      "displayName": "sAPP Factory"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "packagingOptions": {
              "pickFirst": ["**/libc++_shared.so"]
            }
          }
        }
      ],
      ["@lovesworking/watermelondb-expo-plugin-sdk-52-plus"],
      "expo-font",
      "expo-system-ui"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "70142f10-2ded-473e-bb16-cb535d093f5d"
      }
    },
    "owner": "braiancalot"
  }
}
